{
  "name": "unimelb-design-system-assets",
  "description": "Asset compilation using Webpack",
  "version": "6.0.0",
  "repository": "marcom-unimelb/unimelb-design-system",
  "author": "University of Melbourne",
  "license": "UNLICENSED",
  "engines": {
    "node": "6.9.3",
    "npm": "3.10.10"
  },
  "scripts": {
    "prebuild": "rm -Rf build/*",
    "build": "NODE_ENV=production webpack --config assets/webpack.config.js",
    "dev": "node assets/dev-server.js",
    "shrink": "rm -f npm-shrinkwrap.json && npm prune && npm i && npm shrinkwrap --dev",
    "start": "bundle exec foreman start"
  },
  "dependencies": {
    "classlist-polyfill": "unimelb/classlist-polyfill#1.1.20170228",
    "es6-promise": "^4.0.5",
    "has-localstorage": "^1.0.1",
    "just-debounce": "^1.0.0",
    "normalize.css": "^5.0.0",
    "perfect-scrollbar": "^0.6.10",
    "photoswipe": "^4.1.1",
    "promise": "~7.1.1",
    "string-hash": "^1.1.3",
    "webfontloader": "~1.6.3"
  },
  "devDependencies": {
    "babel-core": "~6.17.0",
    "babel-loader": "~6.2.5",
    "babel-preset-es2015": "~6.16.0",
    "css-loader": "~0.25.0",
    "cssesc": "~0.1.0",
    "dotenv": "~2.0.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
    "html-loader": "~0.4.4",
    "imports-loader": "~0.6.3",
    "ip": "~1.1.4",
    "jshint": "~2.9.3",
    "jshint-loader": "~0.8.3",
    "locale-compare-polyfill": "~0.0.2",
    "node-sass": "~3.10.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^2.0.2",
    "postcss-mixins": "^6.0.0",
    "postcss-nested": "^2.0.1",
    "postcss-url": "^6.0.4",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "~4.0.2",
    "style-loader": "~0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "~1.16.1"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
